//
// Inefficient recursive fibonacci
//
// This is the first Soup program ever run. Fun fun!
//
let fibonacci = |x| {
    if x == 0  {
        0
    } else {
        if x == 1 {
            1
        } else {
            fibonacci(x - 1) + fibonacci(x - 2)
        }
    }
}

fibonacci(10)

